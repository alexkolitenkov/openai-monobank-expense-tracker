{
    "appPort": "{{APP_PORT}}",
    "db" : {
        "username": "{{DB_USERNAME}}",
        "password": "{{DB_PASSWORD}}",
        "database": "{{DB_DATABASE}}",
        "dialect": "{{DB_DIALECT}}",
        "host": "{{DB_HOST}}",
        "port": "{{DB_PORT}}"
    },
    "sessions" : {
        "secure" : "{{SESSIONS_SECURE}}",
        "secret":  "{{SECRET}}",
        "name" : "cloud.sid",
        "expires": 43200000
    },
    "mail": {
        "from": "{{MAIL_FROM}}",
        "transport": "{{MAIL_TRANSPORT}}",
        "transportOptions": {
            "host" : "{{MAIL_HOST}}",
            "port" : "{{MAIL_PORT}}",
            "auth" : {
                "user": "{{MAIL_AUTH_USER}}",
                "pass": "{{MAIL_AUTH_PASS}}"
            }
        }
    },
    "currentUrl": "{{CURRENT_URL}}",
    "clientInfoUrl": "{{CLIENT_INFO_URL}}",
    "bankStatementUrl": "{{BANK_STATEMENT_URL}}",
    "encryptionSecret": "{{ENCRYPTION_SECRET}}"
}
